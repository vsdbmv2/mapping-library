{
  "version": "0.0.3",
  "name": "@vsdbmv2/mapping-library",
  "umd:name": "@vsdbmv2/mapping-library",
  "description": "VSDBMv2 mapping methods",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "types/@types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "directory": "@vsdbmv2/mapping-library",
    "url": "https://github.com/vsdbmv2/mapping-library"
  },
  "author": {
    "name": "vsdbmv2",
    "email": "heltonfabio@outlook.com",
    "url": "https://vsdbm.bahia.fiocruz.br"
  },
  "files": [
    "dist",
    "types"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "types": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --cache --fix \"src/**/*.{ts,js}\" ."
  },
  "keywords": [
    "module",
    "node"
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "13.1.3",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "rollup": "2.66.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.27.1",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.8.0",
    "tsm": "2.2.1",
    "typescript": "^4.6.4"
  }
